#include <iostream>

using namespace std;

int main() {
 int x, y;

     x = 7;
     y = 5;
    cout<<"Given x = "<<x<<", y = "<< y<<endl;
    cout<<"x / y produces: "<<x / y<<endl;
    cout<<"(float)x / y produces: " << (float)x / y;
    return 0;

}